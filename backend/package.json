{
  "name": "cod-fraud-backend",
  "version": "1.0.0",
  "description": "COD Fraud Detection & Risk Scoring SaaS - Backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "seed": "tsx src/db/seeds/run.ts",
    "test": "vitest",
    "test:e2e": "vitest --config vitest.e2e.config.ts",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "axios": "^1.6.5",
    "bullmq": "^5.1.6",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.3",
    "fastify": "^4.26.0",
    "libphonenumber-js": "^1.10.54",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^13.1.3",
    "prom-client": "^15.1.3",
    "redis": "^4.6.12",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "drizzle-kit": "^0.20.13",
    "eslint": "^8.56.0",
    "node-pg-migrate": "^7.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
